citHeader("To cite waywiser in publications please use:")

year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{waywiser}: Ergonomic Methods for Assessing Spatial Models",
         author = person("Michael", "Mahoney"),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=waywiser")

 textVersion = paste0("Michael J Mahoney, (", year, ").",
  " waywiser: Ergonomic Methods for Assessing Spatial Models. ",
  note, ".",
  " https://CRAN.R-project.org/package=waywiser")
